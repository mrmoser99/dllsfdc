<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Percent_Selling_of_Financed__c</fullName>
    <description>To get the percentage of Transaction (Financed Amount) = selling price of equipment / financed amt * 100</description>
    <externalId>false</externalId>
    <formula>(Equipment__r.genesis__Estimated_Selling_Price__c  /  Application__r.Financed_Amount_Formula__c)  * 100</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Percent Selling of Financed</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
